              description="Joins additional columns to the data flow by looking up values in a table. For example, join to the 'employee id' column the employees table to get 'hire date' and 'employee name'. We recommend this transformation when the lookup table can fit into memory."
              name="Lookup"
              usesDispositions="true"
              version="6">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement that generates the lookup table."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH CTE AS
(
	SELECT
		REPLACE(ID,'K','') AS RID,
		ID,
		ROW_NUMBER() OVER  (ORDER BY S.ID ASC) AS PRIMARY_KEY
	FROM
		dbo.Subjects(null,100) S
)
--select rid from cte order by rid
SELECT
	CTE.RID AS PARENT_KEY
--,	S.ID CODE
,	value
,	S.HasVariables
,	ROW_NUMBER() OVER  (ORDER BY S.ID ASC) AS PRIMARY_KEY
--,	CTE.PRIMARY_KEY
--,	s.Name
FROM
	CTE
,	dbo.Subjects(null,100) S
	CROSS APPLY STRING_SPLIT(children, ',')
WHERE
	CTE.ID=S.ID</property>
                <property
                  dataType="System.String"
                  description="Specifies a SQL statement that uses parameters to generate the lookup table."
                  expressionType="Notify"
                  name="SqlCommandParam"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select * from (WITH CTE AS
(
	SELECT
		REPLACE(ID,'K','') AS RID,
		ID,
		ROW_NUMBER() OVER  (ORDER BY S.ID ASC) AS PRIMARY_KEY
	FROM
		dbo.Subjects(null,100) S
)
--select rid from cte order by rid
SELECT
	CTE.RID AS PARENT_KEY
--,	S.ID CODE
,	value
,	S.HasVariables
,	ROW_NUMBER() OVER  (ORDER BY S.ID ASC) AS PRIMARY_KEY
--,	CTE.PRIMARY_KEY
--,	s.Name
FROM
	CTE
,	dbo.Subjects(null,100) S
	CROSS APPLY STRING_SPLIT(children, ',')
WHERE
	CTE.ID=S.ID) [refTable]
where [refTable].[PARENT_KEY] = ?</property>
